{
  "name": "web-phone-book-backend",
  "version": "0.1.0",
  "description": "Web Phonebook Backend example with Typescript, Node, and Sequelize",
  "homepage": "https://github.com/iamdanie/nodejs-backend-lab",
  "author": {
    "name": "iamdanie",
    "email": "daniela.bravo@linio.com"
  },
  "main": "build/src/server.js",
  "keywords": [
    "Node",
    "Express",
    "Typescript",
    "Sequelize"
  ],
  "scripts": {
    "build": "gulp build",
    "start": "cross-env NODE_ENV=development gulp watch",
    "start:prod": "cross-env NODE_ENV=production gulp watch",
    "run:test": "cross-env NODE_ENV=test gulp test",
    "tslint": "gulp tslint",
    "precommit": "lint-staged",
    "prettify": "tsfmt -r --baseDir ./src",
    "sqlz:migrate": "NODE_ENV=production ./node_modules/.bin/sequelize db:migrate",
    "sqlz:undo": "NODE_ENV=production ./node_modules/.bin/sequelize db:migrate:undo",
    "sqlz:new": "NODE_ENV=production ./node_modules/.bin/sequelize migration:create"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettify",
      "git add"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.97.0",
    "body-parser": "~1.17.0",
    "cors": "~2.8.1",
    "cross-env": "~5.0.5",
    "express": "~4.15.0",
    "express-boom": "~2.0.0",
    "express-validator": "~3.2.1",
    "fs": "0.0.1-security",
    "morgan": "~1.8.1",
    "multer": "^1.3.0",
    "mysql": "^2.14.1",
    "mysql2": "^1.4.0",
    "sequelize": "~4.4.3",
    "uuid": "~3.1.0",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "~1.16.4",
    "@types/chai": "~4.0.3",
    "@types/express": "~4.0.35",
    "@types/express-validator": "~3.0.0",
    "@types/mocha": "~2.2.39",
    "@types/morgan": "~1.7.32",
    "@types/multer": "^1.3.2",
    "@types/sequelize": "~4.0.68",
    "@types/uuid": "^3.4.4",
    "@types/winston": "~2.3.4",
    "chai": "~4.1.1",
    "extendify": "~1.0.0",
    "glob": "~7.1.1",
    "gulp": "~3.9.1",
    "gulp-istanbul": "~1.1.1",
    "gulp-json-refs": "~0.1.1",
    "gulp-mocha": "~4.3.1",
    "gulp-nodemon": "~2.2.1",
    "gulp-plumber": "~1.1.0",
    "gulp-sourcemaps": "~2.6.0",
    "gulp-tslint": "~8.1.2",
    "gulp-typescript": "~3.2.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "rimraf": "~2.6.1",
    "run-sequence": "~2.1.0",
    "sequelize-cli": "^2.7.0",
    "tslint": "~5.6.0",
    "typescript": "~2.4.2",
    "typescript-formatter": "^5.1.1"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "MIT"
}
